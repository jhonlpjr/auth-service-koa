{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "MS-AUTH Debug",
        "runtimeExecutable": "npm",
        "runtimeArgs": ["run", "dev"],
        "restart": true,
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen",
        "skipFiles": ["<node_internals>/**"],
        "outputCapture": "std",
        "cwd": "${workspaceFolder}"
      }
    ]
  }